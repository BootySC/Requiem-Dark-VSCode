{
	"name": "Requiem Dark",
	"type": "dark",
	"semanticHighlighting": true,
	"git.decorations.enabled": false,
	"colors": {
		"dropdown.background": "#1c1c21",
        "dropdown.border": "#191A21",
        "dropdown.foreground": "#F8F8F2",
        "editorGroupHeader.tabsBackground": "#1c1b1f",
		"foreground": "#F8F8F2",
        "statusBar.background": "#1c1b1f",
        "statusBar.foreground": "#ffffff",
        "titleBar.activeBackground": "#1c1b1f",
        "activityBar.inactiveForeground": "#FFFFFF",
        "activityBar.foreground": "#ffffff",
		"activityBar.background": "#1c1b1f",
		"activityBar.activeBorder": "#4f0074",
		"activityBar.activeBackground": "#1c1b1f",
		"activityBarBadge.foreground": "#FFFFFF",
        "sideBarSectionHeader.background": "#1f1f24",
        "sideBarSectionHeader.border": "#222126",
		"tab.border": "#191A21",
		"sideBar.background": "#1f1f24",
        "tab.activeBorderTop": "#4f0074",
        "tab.inactiveBackground": "#222126",
		"tab.inactiveForeground": "#FFFFFF",
		"editor.background": "#1c1c21",
		"activityBarBadge.background": "#4f0074",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		//Dark Blue
		{
			"name": "Dark Blue",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"new.expr.js",	
				"constant.language.boolean.true.js",
				"constant.language.boolean.false.js"	
			],
			"settings": {
				"foreground": "#4284e2"
			}
		},
		//Light Blue
		{
			"name": "Light Blue",
			"scope": [
				"punctuation.support.type.property-name.begin.json",
				"punctuation.support.type.property-name.end.json",
				"meta.object-literal.key.js",
				"support.variable.property.js",
				//"punctuation.definition.string.begin"
			],
			"settings": {
				"foreground": "#56d2f8"
			}
		},
		//Pink
		{
			"name": "Pink",
			"scope": [
				"keyword.control.loop.js",
				"keyword.control.trycatch.js",
				"keyword.control.conditional.js",
				"support.type.object.module.js",
				"keyword.control.flow.js",
			],
			"settings": {
				"foreground": "#f856e3"
			}
		},
		//White
		{
			"name": "White",
			"scope": [
				"keyword.operator.comparison.js",
				"keyword.operator.logical.js",
				"punctuation",
				"meta.brace.round.js"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#56d2f8"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				//"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#4284e2"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				//"keyword.control",
				//"constant.other.color",
				//"punctuation",
				//"meta.tag",
				//"punctuation.definition.tag",
				//"punctuation.separator.inheritance.php",
				//"punctuation.definition.tag.html",
				//"punctuation.definition.tag.begin.html",
				//"punctuation.definition.tag.end.html",
				//"punctuation.section.embedded",
				//"keyword.other.template",
				//"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Tag",
